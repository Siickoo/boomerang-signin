<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Boomerang Cardiff Sign In/Out</title>
    <style>
        body { font-family: Arial, sans-serif; text-align: center; margin: 20px; font-size: 20px; }
        button { margin: 10px; padding: 15px 30px; font-size: 20px; display: inline-block; }
        input, select { font-size: 18px; padding: 5px; }
        #signInDiv, #signOutDiv { display: none; margin-top: 20px; }
        #whoIsInList { margin-top: 20px; font-size: 22px; }
        img { max-width: 400px; display: block; margin: 0 auto 20px; } /* Increased logo size */
    </style>
</head>
<body>
    <img src="data:image/png;base64," alt="Boomerang Cardiff Logo">
    <h2>Boomerang Cardiff Sign In/Out</h2>
    <button onclick="showSignIn()">SIGN IN</button>
    <button onclick="showSignOut()">SIGN OUT</button>
    <button onclick="showWhosIn()">WHO'S IN</button>
    
    <div id="signInDiv">
        <input type="text" id="nameInput" placeholder="Enter your name">
        <button onclick="signIn()">Submit</button>
    </div>
    
    <div id="signOutDiv">
        <select id="signOutSelect"></select>
        <button onclick="signOut()">Submit</button>
    </div>
    
    <div id="whoIsInList"></div>
    
    <script>
        function getSignedInUsers() {
            return JSON.parse(localStorage.getItem("signedInUsers")) || [];
        }
        
        function saveSignedInUsers(users) {
            localStorage.setItem("signedInUsers", JSON.stringify(users));
        }
        
        function showSignIn() {
            document.getElementById("signInDiv").style.display = "block";
            document.getElementById("signOutDiv").style.display = "none";
        }
        
        function showSignOut() {
            let users = getSignedInUsers();
            let select = document.getElementById("signOutSelect");
            select.innerHTML = "";
            users.forEach(user => {
                let option = document.createElement("option");
                option.textContent = user;
                select.appendChild(option);
            });
            
            document.getElementById("signInDiv").style.display = "none";
            document.getElementById("signOutDiv").style.display = users.length ? "block" : "none";
        }
        
        function showWhosIn() {
            let users = getSignedInUsers();
            document.getElementById("whoIsInList").innerHTML = users.length ? "<b>Currently Signed In:</b><br>" + users.join("<br>") : "No one is currently signed in.";
        }
        
        function signIn() {
            let name = document.getElementById("nameInput").value.trim();
            if (name) {
                let users = getSignedInUsers();
                if (!users.includes(name)) {
                    users.push(name);
                    saveSignedInUsers(users);
                    alert(name + " signed in successfully!");
                } else {
                    alert("You're already signed in!");
                }
            }
            document.getElementById("nameInput").value = "";
        }
        
        function signOut() {
            let select = document.getElementById("signOutSelect");
            let selectedName = select.value;
            let users = getSignedInUsers().filter(user => user !== selectedName);
            saveSignedInUsers(users);
            alert(selectedName + " signed out successfully!");
            showSignOut();
        }
    </script>
</body>
</html>
